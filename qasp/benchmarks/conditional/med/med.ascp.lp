	holds(bsd,T,P),
	holds(neg(dead),T,P).
poss(medicate(1),T,P) :-
	holds(neg(dead),T,P).
poss(medicate(2),T,P) :-
	holds(neg(dead),T,P).
poss(medicate(3),T,P) :-
